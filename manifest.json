{
  "name": "Mspaintadventures.ru flash changer",
  "description": "Подменивает флеш на страницах mspaintadventures.ru с флешем на видео с ютуба!",
  "version": "1.2",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources":[
    {
      "matches":["<all_urls>"],
      "resources":["pages.json","mobileStyle.css","fonts/CourierNew.ttf","fonts/Verdana.ttf","fonts/Helvetica.otf","fonts/Arial.ttf","fonts/SansSerif.ttf"]
    }
  ],
  "content_scripts":[
    {
      "matches":["https://www.mspaintadventures.ru/?s=6&p=*"],
      "js":["flashChanger.js","fontChangerMobile.js"]
    }
  ],
  "icons": {
    "16":"images/popup16.png",
    "32":"images/popup32.png",
    "48":"images/popup48.png",
    "128":"images/popup128.png"
  }
}